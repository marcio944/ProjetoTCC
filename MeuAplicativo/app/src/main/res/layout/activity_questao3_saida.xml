<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EntradaSaída.Questao3SaidaActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="16dp">

        <TextView
            android:id="@+id/textView98"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="19dp"
            android:layout_marginLeft="19dp"
            android:layout_marginTop="22dp"
            android:text="Complete o Pseudocódigo abaixo com os comandos que estão faltando."
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView99"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="74dp"
            android:text="algoritmo 'soma'
        \n\nvar n1, n2: inteiro
        \n\ninicio
        \n\n                 ('Digite o primeiro valor')
        \n\n                 leia(n1)
        \n \n                 ('Digite o segundo valor')
        \n\n                  leia (n1)
        \n\n                  ('Resultado = ' ,n1+n2)
        \n\nfimalgoritmo"
            android:textStyle="bold" />

        <Button
            android:id="@+id/button163"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="34dp"
            android:layout_marginBottom="43dp"
            android:background="@color/colorPrimary"
            android:onClick="anterior"
            android:text="Anterior"
            android:textColor="#ffffff" />

        <Button
            android:id="@+id/button164"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/imageButton81"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="35dp"
            android:background="@color/colorPrimary"
            android:onClick="proximo"
            android:text="Próximo"
            android:textColor="#ffffff" />

        <ImageButton
            android:id="@+id/imageButton81"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_alignBottom="@+id/button163"
            android:layout_centerHorizontal="true"
            android:background="@null"
            android:onClick="inicio"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/home" />

        <EditText
            android:id="@+id/editText3"
            android:layout_width="68dp"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView99"
            android:layout_alignParentTop="true"
            android:layout_marginStart="-11dp"
            android:layout_marginTop="160dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:textSize="15dp"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/editText4"
            android:layout_width="67dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/editText3"
            android:layout_alignStart="@+id/textView99"
            android:layout_marginStart="-8dp"
            android:layout_marginTop="20dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:textSize="15dp"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/editText5"
            android:layout_width="71dp"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView99"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="-9dp"
            android:layout_marginBottom="235dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:textSize="15dp"
            android:textStyle="bold" />
    </RelativeLayout>
</android.support.constraint.ConstraintLayout>